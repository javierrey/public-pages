{
  "name": "public-pages",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "description": "Public pages monorepo",
  "author": "JR <javier.rey.eu@gmail.com> (https://javierrey.github.io)",
  "workspaces": ["packages/*", "apps/*"],
  "scripts": {
    "git:init": "[ ! -d './.git' ] && git init --initial-branch=main",
    "status": "echo CLEAN: && git clean -nfdX -e !.* -e !**/_ignore/ -e !**/_ignore/** -e !*.ignore.* -e !local_modules/ -e !local_modules/** && echo NEW: && git ls-files -o --exclude-standard",
    "clean": "git clean -fdX -e !.* -e !**/_ignore/ -e !**/_ignore/** -e !*.ignore.* -e !local_modules/ -e !local_modules/**",
    "install": "npm run clean && npm install",
    "build": "echo '@todo build'",
    "build:update-pages": "npm run build && node ./scripts/update-pages.js",
    "start": "echo '@todo start'",
    "test": "echo '@todo test'"
  },
  "dependencies": {},
  "devDependencies": {},
  "ignore": {
    "dependencies": {},
    "devDependencies": {}
  }
}
